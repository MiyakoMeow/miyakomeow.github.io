{
  "name": "miyakomeow.github.io",
  "version": "1.0.0",
  "description": "Personal website built with Vue 3 and Vite",
  "type": "module",
  "scripts": {
    "dev": "bun run vite",
    "build": "vue-tsc --noEmit && bun run vite build",
    "preview": "bun run vite preview",
    "serve": "bun run preview",
    "type-check": "vue-tsc --noEmit",
    "type-check:watch": "vue-tsc --noEmit --watch",
    "lint": "eslint . --ext .js,.ts,.vue",
    "lint:fix": "eslint . --ext .js,.ts,.vue --fix",
    "format": "prettier . --write",
    "format:check": "prettier . --check"
  },
  "dependencies": {
    "vue": "^3.5.25"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@tailwindcss/vite": "^4.1.17",
    "@types/node": "^24.10.1",
    "@vitejs/plugin-vue": "^6.0.2",
    "@vue/eslint-config-typescript": "^14.6.0",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-vue": "^10.6.2",
    "globals": "^16.5.0",
    "prettier": "^3.7.4",
    "tailwindcss": "^4.1.17",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.48.1",
    "vite": "^7.2.6",
    "vue-eslint-parser": "^10.2.0",
    "vue-tsc": "^3.1.7"
  },
  "engines": {
    "bun": ">=1.0.0"
  },
  "keywords": [
    "vue",
    "vite",
    "personal-website"
  ],
  "author": "",
  "license": "MIT"
}
