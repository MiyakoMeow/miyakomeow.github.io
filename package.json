{
  "name": "miyakomeow.github.io",
  "version": "1.0.0",
  "description": "Personal website built with Svelte and Vite",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "svelte-check --tsconfig ./tsconfig.json --diagnostic-sources \"js,svelte\" --compiler-warnings \"css_unused_selector:ignore\" && vite build",
    "preview": "vite preview",
    "type-check": "svelte-check --tsconfig ./tsconfig.json --diagnostic-sources \"js,svelte\" --compiler-warnings \"css_unused_selector:ignore\"",
    "type-check:watch": "svelte-check --tsconfig ./tsconfig.json --diagnostic-sources \"js,svelte\" --compiler-warnings \"css_unused_selector:ignore\" --watch",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier . --write",
    "format:check": "prettier . --check"
  },
  "dependencies": {
    "svelte": "^5"
  },
  "devDependencies": {
    "@eslint/css": "^0.14.1",
    "@eslint/js": "^9.39.1",
    "@sveltejs/vite-plugin-svelte": "^6.2.1",
    "@tailwindcss/postcss": "^4.1.17",
    "@tailwindcss/vite": "^4.1.17",
    "@types/node": "^25.0.0",
    "autoprefixer": "^10.4.22",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-better-tailwindcss": "^3.8.0",
    "globals": "^16.5.0",
    "postcss": "^8.5.6",
    "postcss-nesting": "^13.0.2",
    "prettier": "^3.7.4",
    "svelte-check": "^4.3.4",
    "tailwind-csstree": "^0.1.4",
    "tailwindcss": "^4.1.17",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.48.1",
    "vite": "^7.2.6"
  },
  "engines": {
    "bun": ">=1.0.0"
  },
  "keywords": [
    "svelte",
    "vite",
    "personal-website"
  ],
  "author": "",
  "license": "MIT"
}
